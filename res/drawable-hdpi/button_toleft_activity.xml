<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" android:drawable="@drawable/arrow_left_pressed" >
        <shape>
            <gradient  
        		android:startColor="#545454"  
        		android:endColor="#242424"   
        		android:angle="270"  />  



        </shape>
    </item>



    <item android:drawable="@drawable/arrow_left" >       
        <shape>
            <solid
                android:color="#1E1E1E"  />


        </shape>
    </item>

</selector>
